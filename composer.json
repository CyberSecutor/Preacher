{
  "name": "zero-config/preacher",
  "description": "Personal static website generator",
  "homepage": "http://janmarten.name",
  "type": "project",
  "keywords": ["blog", "website", "php", "twig", "markdown", "git"],
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "bin/preach"
  ],
  "scripts": {
    "preach": "bin/preach"
  },
  "archive": {
    "exclude": [
      "phpunit.xml",
      "tests"
    ]
  },
  "authors": [
    {
      "name": "Jan-Marten de Boer",
      "email": "github@johmanx.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ZeroConfig\\Preacher\\": "src"
    }
  },
  "require": {
    "php": "^7.0",
    "erusev/parsedown": "^1.6.1",
    "twig/twig": "^2.1.0",
    "coyl/git": "^0.1.4.9",
    "composer/composer": "^1.3.2"
  },
  "require-dev": {
    "raveren/kint": "^1.0.6"
  }
}
